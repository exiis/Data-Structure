<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:controller="SimpleDatabase.loadController" prefHeight="137.0" prefWidth="310.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Pane layoutX="10.0" layoutY="16.0" prefHeight="143.0" prefWidth="291.0">
         <children>
            <TextField fx:id="tfName" layoutX="70.0" layoutY="58.0" prefHeight="22.0" prefWidth="180.0" promptText="Input File Name" />
            <Label layoutX="5.0" layoutY="58.0" prefHeight="22.0" prefWidth="60.0" text="FileName :" />
            <Label layoutX="250.0" layoutY="58.0" prefHeight="22.0" prefWidth="28.0" text=".dat" />
            <Label layoutX="118.0" layoutY="-5.0" prefHeight="38.0" prefWidth="84.0" text="Load">
               <font>
                  <Font size="26.0" />
               </font>
            </Label>
            <Button layoutX="160.0" layoutY="102.0" mnemonicParsing="false" text="Load" onAction="#load" />
            <Button fx:id="btCancel" layoutX="212.0" layoutY="102.0" mnemonicParsing="false" text="Cancel" onAction="#close" />
			<Label fx:id="lbState" layoutY="106.0" prefHeight="22.0" prefWidth="140.0" text="save..." />
            
         </children>
      </Pane>
   </children>
</AnchorPane>
