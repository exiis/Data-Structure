<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="137.0" prefWidth="310.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SimpleDatabase.saveController">
   <children>
      <Pane layoutX="10.0" layoutY="16.0" prefHeight="143.0" prefWidth="291.0">
         <children>
            <TextField fx:id="tfName" layoutX="70.0" layoutY="58.0" prefHeight="22.0" prefWidth="180.0" promptText="Input File Name" />
            <Label layoutX="5.0" layoutY="58.0" prefHeight="22.0" prefWidth="60.0" text="FileName :" />
            <Label layoutX="250.0" layoutY="58.0" prefHeight="22.0" prefWidth="28.0" text=".dat" />
            <Label layoutX="118.0" layoutY="-5.0" prefHeight="38.0" prefWidth="84.0" text="Save">
               <font>
                  <Font size="26.0" />
               </font>
            </Label>
            <Button layoutX="160.0" layoutY="102.0" mnemonicParsing="false" onAction="#save" text="Save" />
            <Button fx:id="btCancel" layoutX="212.0" layoutY="102.0" mnemonicParsing="false" onAction="#close" text="Cancel" />
            <Label fx:id="lbState" layoutY="106.0" prefHeight="22.0" prefWidth="140.0" text="save..." />
         </children>
      </Pane>
   </children>
</AnchorPane>
